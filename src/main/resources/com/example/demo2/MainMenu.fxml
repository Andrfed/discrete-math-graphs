               <image>
                  <Image url="@logo.png" />
               </image>
            </ImageView>
            <Label layoutX="18.0" layoutY="17.0" prefHeight="20.0" prefWidth="233.0" text="Выбор задачи">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <MenuButton layoutX="18.0" layoutY="51.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="233.0" style="-fx-font: normal 20px 'serif';" text="1">
              <items>
                <MenuItem mnemonicParsing="false" onAction="#onTo1" text="1" />
                <MenuItem mnemonicParsing="false" onAction="#onTo2" text="2" />
                <MenuItem mnemonicParsing="false" onAction="#onTo3" text="3" />
