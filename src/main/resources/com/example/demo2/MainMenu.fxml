                <MenuItem mnemonicParsing="false" onAction="#onTo7" text="7" />
                <MenuItem mnemonicParsing="false" onAction="#onTo8" text="8" />
                <MenuItem mnemonicParsing="false" onAction="#onTo9" text="9" />
                <MenuItem mnemonicParsing="false" onAction="#onTo10" text="10" />
                <MenuItem mnemonicParsing="false" onAction="#onTo11" text="11" />
                <MenuItem mnemonicParsing="false" onAction="#onTo12" text="12" />
              </items>
            </MenuButton>
            <ListView fx:id="ListView1" layoutX="18.0" layoutY="145.0" prefHeight="200.0" prefWidth="233.0" />
      </children>
    </AnchorPane>
  </children>
